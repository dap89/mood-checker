mood = input("How do you feel today: happy, nervous, sad, excited or relaxed? ->")

if mood == "happy":
    print("It is great to see you happy!")
elif mood == "nervous":
    print("Take a deep breath 3 times.")
elif mood == "sad":
    print("Is there anything I can do for you?")
elif mood == "excited":
    print("Great!")
elif mood == "relaxed":
    print("Go on!")
else:
    print("Sorry, I don't recognize this mood.")
